{
    "name": "healthvia",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "dev:safe": "rimraf .next && next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --check 'src/**/*.{js,ts,jsx,tsx,mjs,cjs,mts,cts,css}'",
        "format:fix": "prettier --write 'src/**/*.{js,ts,jsx,tsx,mjs,cjs,mts,cts,css}'",
        "lint:fix": "eslint . --fix",
        "typecheck": " tsc --noEmit -p tsconfig.json --composite false"
    },
    "dependencies": {
        "next": "15.3.2",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "eslint": "^9",
        "eslint-config-next": "15.3.2",
        "prettier": "^3.5.3",
        "prettier-plugin-organize-imports": "^4.1.0",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "rimraf": "^6.0.1",
        "tailwindcss": "^4",
        "typescript": "^5"
    }
}
